
score = input("Enter Score: ")
try:
    value = float(score)  
except:
    print("ERROR")
    
if value > 1:
    print('Its not possible')
   
elif value >= 0.9:
    print('A')
    
elif value >= 0.8:
    print('B')
    
elif value >= 0.7:
    print('C')

elif value >= 0.6:
    print('D')

elif value < 0.6:
    print('F')

   
    
